#include <iostream>
using namespace std;

int main()
{
    int a,b;

    // read
    cout<<"Enter number a: "; cin>>a;
    cout<<"Enter number b: "; cin>>b;

    // display without aux

    cout<<"Number a is: "<<b<<endl;
    cout<<"Number b is: "<<a<<endl;

    //display with aux

    int aux;
    aux = a;
    a = b;
    b = aux;

    cout<<"Number a is: "<<a<<endl;
    cout<<"Number b is: "<<b<<endl;

    return 0;
}
